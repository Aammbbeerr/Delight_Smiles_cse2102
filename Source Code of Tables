Tables:

CREATE TABLE ` lcd dental record `.`Patient` ( 
	`Patient ID` INT(7) NOT NULL,
	`First Name` VARCHAR(15) NOT NULL,
	`Last Name` VARCHAR(15) NOT NULL,
	`Date of Birth` DATE NOT NULL,
	`Sex` CHAR(1) NOT NULL,
	`Address` VARCHAR(120) NOT NULL,
	`Teeth Number` INT(7) NOT NULL,
	`Note` VARCHAR(100) NULL,
	PRIMARY KEY(`Patient ID`) );

CREATE TABLE `lcd dental record`.`Dentist` ( 
	`Dentist ID` INT(7) NOT NULL,
	`First Name` VARCHAR(15) NOT NULL,
	`Last Name` VARCHAR(15) NOT NULL,
	`Sex` CHAR(1) NOT NULL,
	`Note` VARCHAR(100) NULL,
	PRIMARY KEY(`Dentist ID`) ); 

CREATE TABLE `lcd dental record`.`Teeth` ( 
	`Teeth ID` INT(7) NOT NULL,
	`Number of Anesthesia` INT(1) NULL,
	PRIMARY KEY(`Teeth ID`) );
  
